@using MvpSite.Rendering.Models
@model MvpSite.Rendering.Models.TopLinks

<nav class="navbar navbar-expand p-0" id="topbar">
    <div class="container">
        <div class="nav navbar-nav d-none d-md-flex col-md-9 inner py-0">
            @foreach (TopLink topLink in Model.Items ?? [])
            {
                <a class="nav-item nav-link" target="_blank" href="@topLink.Fields?.Link?.Value.Href">@topLink.Fields?.Link?.Value.Text</a>
            }
        </div>
        <sc-placeholder name="header-signin"></sc-placeholder>
    </div>
</nav>