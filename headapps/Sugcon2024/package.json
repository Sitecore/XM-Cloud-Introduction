    "@sitecore-jss/sitecore-jss-nextjs": "~22.9.0",
    "@sitecore/engage": "^1.4.3",
    "@types/react-slick": "^0.23.13",
    "bootstrap": "^5.1.3",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.1.0",
    "dangerously-set-html-content": "^1.1.0",
    "font-awesome": "^4.7.0",
    "framer-motion": "^11.0.0",
    "graphql": "~15.8.0",
